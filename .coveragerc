[run]
branch = True
omit =
    ghcontrib/tests/*
    ghcontrib/migrations/*
    /tmp/*
    /usr/*
    /opt/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError

ignore_errors = True
show_missing = True

omit =
    ghcontrib/tests/*
    ghcontrib/migrations/*
    /tmp/*
    /usr/*
    /opt/*
